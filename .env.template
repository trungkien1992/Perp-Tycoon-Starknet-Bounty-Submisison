# Extended API Configuration
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env file to version control

# Extended Exchange API Key (get from Extended Exchange dashboard)
EXTENDED_API_KEY=your_api_key_here

# Extended Exchange Vault ID (your trading vault identifier)
EXTENDED_VAULT_ID=your_vault_id_here

# Extended API Endpoints (default values, modify if needed)
EXTENDED_API_BASE_URL=https://extended.exchange/api
EXTENDED_API_WS_URL=wss://extended.exchange/ws

# Starknet Configuration
# Your Starknet private key (64 characters, with 0x prefix)
STARKNET_PRIVATE_KEY=0x_your_64_character_private_key_here

# Your Starknet public key (64 characters, with 0x prefix)  
STARKNET_PUBLIC_KEY=0x_your_64_character_public_key_here

# App Configuration
APP_MODE=real